g++ -DASTEROIDS_MAX_J=0 -DASTEROIDS_MAX_K=2 -DNAME=asteroids_0_2 -O3 -Wall -shared -std=c++11 -fPIC $(python3 -m pybind11 --includes) main.cpp ../sim/*.cpp -o asteroids_0_2$(python3-config --extension-suffix)
g++ -DASTEROIDS_MAX_J=0 -DASTEROIDS_MAX_K=3 -DNAME=asteroids_0_3 -O3 -Wall -shared -std=c++11 -fPIC $(python3 -m pybind11 --includes) main.cpp ../sim/*.cpp -o asteroids_0_3$(python3-config --extension-suffix)
